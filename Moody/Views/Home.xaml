<?xml version="1.0" encoding="UTF-8"?> 
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="Moody.Views.Home" Title="Moody">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="MOODY" HorizontalOptions="Center" TextColor="#FAD102" Margin="0,80,0,0" FontSize="50.0"/>
            <ImageButton Source="button-newMood.png" HorizontalOptions="Center" VerticalOptions="Center" WidthRequest="180" Margin="0,50,0,0" Clicked="OnButtonClickNewMood"/>
            <ImageButton Source="button-history.png" HorizontalOptions="Center" VerticalOptions="Center" WidthRequest="180" Margin="0,50,0,0" Clicked="OnButtonClickHistory"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>
